{
    "name": "deawx/cyberrestapi",
    "description": "Fast PHP API Framework",
    "type": "project",
    "license": "MIT",
    "homepage": "https://cyberthai.net",
    "keywords": [
        "php framework",
        "rest api",
        "restapi",
        "php",
        "php-api",
        "api-framework",
        "framework",
        "mvc"
    ],
    "autoload": {
        "psr-4": {
            "App\\": "App/",
            "Core\\": "Core/"
        }
    },
    "authors": [
        {
            "name": "(deawx) Tirapong Chaiyakun",
            "email": "msdos43@gmail.com",
            "homepage": "https://cyberthai.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2 || ^8.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-mbstring": "*",
        "catfan/medoo": "^2.1",
        "fakerphp/faker": "^1.24",
        "filp/whoops": "^2.15",
        "firebase/php-jwt": "^6.10",
        "guzzlehttp/guzzle": "^7.9",
        "predis/predis": "^2.2",
        "symfony/console": "^7.1",
        "symfony/process": "^7.1",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}